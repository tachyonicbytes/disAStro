    "as-uuid": "^0.0.4",
    "assemblyscript": "^0.25.0",
    "assemblyscript-regex": "^1.6.4",
    "byte-parse-as": "^0.1.0",
    "json-as": "^0.5.3",
    "visitor-as": "^0.11.4"
  },
  "exports": {
    ".": {
      "import": "./build/release.js",
      "types": "./build/release.d.ts"
    }
  },
  "devDependencies": {
    "@as-pect/cli": "^8.1.0"
  },
  "repository": {
    "type": "git",
